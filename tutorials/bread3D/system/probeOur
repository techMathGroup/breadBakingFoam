FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       dictionary;
    location    "system";
    object      probe2D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

probeOur
{
    // Mandatory entries
    type                probes;
    libs                (sampling);
    probeLocations      ((0.0 0 0) (0.0 0.034 0) (17e-3 0 0));
    fields              (T D);
}