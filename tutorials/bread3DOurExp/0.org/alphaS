FoamFile
{
    version     1.0;
    format      ascii;
    class       volScalarField;
    object      alphaS;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.483;

boundaryField
{
	"(sides|bottom|top)"
	{
        type zeroGradient;
	}
	
  "symmetryPatch"
  {
    type  symmetry;
  }
  "(wedgeZ0|wedgeZE)"
	{
		type wedge;
	}
}

