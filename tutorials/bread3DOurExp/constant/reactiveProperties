// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
// Dictionary for right hand side of thermDCRRhoPorousSimpleFOAM

/* 
    Prepared kinetics setup:
        1) CO oxidation -- inhibition kinetics accroding to 3D reconstruction and pore-scale modeling of coated catalytic filters for automotive exhaust gas aftertreatment (Koci 17)
        2) general power law kinetics -- r = \Pi_{\all i} k*c_i^alpha_i
        ==> switch using kinSetupSwitch 1 or 2
*/

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

reactProps
{
	evaporation
	{
        dH dH [0 2 -2 0 0 0 0] -2.38318e+06;
		kMPCOpen kMPCOpen [0 0 -1 0 0 0 0] 0.42;
		kMPCClosed kMPCClosed [0 0 -1 0 0 0 0] 0.42;
		evCoef1 evCoef1 [0 0 0 0 0 0 0] -0.0056;
		evCoef2 evCoef2 [0 0 0 0 0 0 0] 5.5;

        // effectively not used (same kMPC for all temperatures)
        TOpen TOpen [0 0 0 0 0 0 0] 75;
        deltaTOpen deltaTOpen [0 0 0 0 0 0 0] 10;
	}
    fermentation
	{
		R0 R0 [1 -3 -1 0 0 0 0] 0.0022;
		Tm Tm [0 0 0 1 0 0 0] 314;
		deltaT deltaT [0 0 0 1 0 0 0] 10;
	}
}

