FoamFile
{
    version     1.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian;

// ************************************************************************* //
// -- general constants
genProps
{
    univR   univR   [1  2 -2 -1 -1 0 0]  8.31446261815324;
}

// ************************************************************************* //
withDeformation 1;

withEq 1;

permGLViscG permGLViscG [-1 3 1 0 0 0 0] 9e-13;
