FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default            steadyState;
}


ddtSchemes
{
    default            Euler;
    d2dt2(D_0)         steadyState;
    d2dt2(D)            steadyState;
    d2dt2(rho,D_0)      steadyState;
}

gradSchemes
{
    default            Gauss linear;
}

divSchemes
{
    default                             Gauss linear;
}

laplacianSchemes
{
    default            Gauss linear corrected;
}

snGradSchemes
{
    default            corrected;
}

interpolationSchemes
{
    default            linear;
}
fluxRequired
{
    default         no;
    p;
}

